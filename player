package knfhjk;

public class game {
    
    
       

        
        String nama;
        int speed;
        int healthPoin;
        int dmg;
        int shield;
    
        
        void run(){
            System.out.println(nama +" is running...");
            System.out.println("Speed: "+ speed);
        }
    
       
        boolean isDead(){
            if(healthPoin <= 0) return true;
            return false;
        }
    }   

